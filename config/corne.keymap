#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#define DVORAK 0 //added these in to define the layers.
#define FN 1
#define NUM 2

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
// -----------------------------------------------------------------------------------------
// |  '  |  ,  |  .  |  P  |  Y  |   |    F   |  G   |  C  |  R  |  L  |
// |  A  |  O  |  E  |  U  |  I  |   |    D   |  H   |  T  |  N  |  S  |
// | LS ; |  Q  |  J  |  K  |  X  |   |    B   |  M   |  W  |  V  | RS Z  |
//             | GUI | NUM TAB | ENT|   | FN-BSPC | SPC | DEL |
                        bindings = <
   &none &kp Q	&kp W	&kp E &kp R &kp T  	&kp Y	&kp U	&mt RG(C) I &kp O   &kp P    &none
   &none &kp A &kp S 	&kp D &kp F &kp G  	&kp H 	&kp J	&kp K   &kp L   &kp SEMI &none
   &none &mt LSHIFT Z 	&kp X 	&kp C 	&kp V	&mt RG(X) B &kp N 	&kp M	&kp COMMA	&mt RG(V) DOT &mt RSHIFT FSLH &none
                &kp LGUI &lt NUM TAB &kp RET	&lt FN BSPC &kp SPACE	&kp DEL
                        >;
                };
                lower_layer {
// -----------------------------------------------------------------------------------------
// | ESC  |     | F10  | F11  | F12  |   |     | HOME |  UP  | END | gu\ |
// | CRTL |     | F7   | F8   | F9   |   |  TAB   | LFT  |  DN  | RGT | PGUP |
// | LS   | OPT | F1-4 | F2-5 | F3-6 |   |     |      |     |     |  PGDN   |
//                   | GUI | NUM TAB | ENT|   | FN-BSPC | SPC | DEL |
                        bindings = <
   &none &kp ESC    &none   	&kp F10    &kp F11   &kp F12	    &none   &kp RG(LEFT)  &kp UP   	&kp RG(RIGHT)	  &kp RG(BSLH) &none
   &none &kp LCTRL &none	 	&kp F7     &kp F8	 &kp F9   		&kp TAB	&kp LEFT  &kp DOWN  &kp RIGHT &kp PG_UP &none
   &none &kp LSHIFT  &kp LALT  &mt F1 F4  &mt F2 F5 &mt F3 F5      &none	&none     &none 	&none     &kp PG_DN &none
                &kp LGUI &lt NUM TAB &kp RET	&lt FN BSPC &kp SPACE	&kp DEL
                        >;
                };

                raise_layer {
// -----------------------------------------------------------------------------------------
// |  `~  |  <  |  >  |  (  |  )  |  |  &  |   7  |  8  |  9  |  /?  |
// |      |  ,  |  .  |  {  |  }  |  | \|  |   4  |  5  |  6  |  =+  |
// |  LS  |  *  |  ^  |  [  |  ]  |  |  0  |   1  |  2  |  3  |  -_  |
//              | GUI | NUM TAB | ENT|   | FN-BSPC | SPC | DEL |
                        bindings = <
   &none &kp GRAVE &kp LS(W)		&kp	LS(V)	  &kp LPAR 	&kp	RPAR	  &kp AMPS	   &kp N7	&kp N8	&kp N9	&kp LBKT &none
   &none &none &kp W	&kp	V	  &kp UNDER 	&kp	PLUS	  &kp BSLH	   &kp N4	&kp N5	&kp N6 	&kp RBKT &none
   &none &kp LSHIFT	 &kp STAR	&kp CARET &kp MINUS 	&kp	EQUAL	  &kp N0 	   &kp N1	&kp N2	&kp N3	&kp APOS &none
             	&kp LGUI &lt NUM TAB &kp RET	&lt FN BSPC &kp SPACE	&kp DEL
                        >;
                };
        };
};
