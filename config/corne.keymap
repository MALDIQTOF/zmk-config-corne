#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#define DVORAK 0 //added these in to define the layers.
#define FN 1
#define NUM 2

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
// -----------------------------------------------------------------------------------------
// |  '  |  ,  |  .  |  P  |  Y  |   |    F   |  G   |  C  |  R  |  L  |
// |  A  |  O  |  E  |  U  |  I  |   |    D   |  H   |  T  |  N  |  S  |
// | LS ; |  Q  |  J  |  K  |  X  |   |    B   |  M   |  W  |  V  | RS Z  |
//             | GUI | TAB | NUM ENT|   | FN-SPC | BSPC | DEL |
                        bindings = <
   &none &kp APOS	&kp COMMA	&kp DOT &kp P 	&kp Y   	&kp F		&kp G	&mt RG(C) C &kp R   &kp L    &none
   &none &kp A &kp O 	&kp E 	&kp U 	&kp I   	&kp D 		&kp H	&kp T   &kp N   &kp S &none
   &none &mt LSHIFT SEMI 	&kp Q 	&kp J 	&kp K	&mt RG(X) X &kp B 	&kp M	&kp W	&mt RG(V) V &mt RSHIFT Z &none
                &kp LGUI &lt NUM RET &kp TAB	&kp BSPC &lt FN SPACE	&kp DEL
                        >;
                };
                lower_layer {
// -----------------------------------------------------------------------------------------
// | ESC  |     | F10  | F11  | F12  |   |     | HOME |  UP  | END | PGUP |
// | CRTL |     | F7   | F8   | F9   |   |     | LFT  |  DN  | RGT | PGDN |
// | LS   | OPT | F1-4 | F2-5 | F3-6 |   |     |      |     |     |     |
//                   | GUI | TAB | NUM ENT |   | FN-SPC | BSPC | DEL |
                        bindings = <
   &none &kp ESC    &none   	&kp F10    &kp F11   &kp F12	    &none   &kp HOME  &kp UP   	&kp END	  &kp PG_UP &none
   &none &kp LCTRL &none	 	&kp F7     &kp F8	 &kp F9   		&none	&kp LEFT  &kp DOWN  &kp RIGHT &kp PG_DN &none
   &none &kp LSHIFT  &kp LALT  &mt F1 F4  &mt F2 F5 &mt F3 F5      &none	&none     &none 	&none     &none &none
                &kp LGUI &lt NUM RET &kp TAB	&kp BSPC &lt FN SPACE	&kp DEL
                        >;
                };

                raise_layer {
// -----------------------------------------------------------------------------------------
// |  `~  |  <  |  >  |  (  |  )  |  |  &  |   7  |  8  |  9  |  /?  |
// |      |  ,  |  .  |  {  |  }  |  | \|  |   4  |  5  |  6  |  =+  |
// |  LS  |  *  |  ^  |  [  |  ]  |  |  0  |   1  |  2  |  3  |  -_  |
//              | GUI | TAB | NUM ENT |  | SPC | BSPC | *\ |
                        bindings = <
   &none &kp GRAVE &kp LT		&kp	GT	  &kp LPAR 	&kp	RPAR	  &kp AMPS	   &kp N7	&kp N8	&kp N9	&kp FSLH &none
   &none &none &kp COMMA	&kp	DOT	  &kp LBRC 	&kp	RBRC	  &kp BSLH	   &kp N4	&kp N5	&kp N6 	&kp EQUAL &none
   &none &kp LSHIFT	 &kp STAR	&kp CARET &kp LBKT 	&kp	RBKT	  &kp N0 	   &kp N1	&kp N2	&kp N3	&kp MINUS &none
             	&kp LGUI &lt NUM RET &kp TAB	&kp BSPC &lt FN SPACE	&kp DEL
                        >;
                };
        };
};
